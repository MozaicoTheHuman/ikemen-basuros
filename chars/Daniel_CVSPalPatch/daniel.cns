trigger1 = Time = 10
value = 100

[State 105, 3]
type = MakeDust
trigger1 = Time >= 10
pos = -5,-2
spacing = 3

[State 100, 1]
type = VelSet
trigger1 = Time >= 10
x = const(velocity.run.fwd.x)

[State 100, 2] ;Prevent run from canceling into walk
type = AssertSpecial
trigger1 = 1
flag = NoWalk

[State 100, 3] ;Prevent from turning
type = AssertSpecial
trigger1 = 1
flag = NoAutoTurn

[State 100, 4]
type = ChangeState
trigger1 = command != "holdfwd"
value = 0

